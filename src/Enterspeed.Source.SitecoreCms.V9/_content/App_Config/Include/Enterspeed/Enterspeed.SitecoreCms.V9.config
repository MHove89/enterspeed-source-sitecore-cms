<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore>
    <services>
      <configurator type="Enterspeed.Source.SitecoreCms.V9.DependencyInjection.ServicesConfigurator, Enterspeed.Source.SitecoreCms.V9" resolve="true" />
    </services>

    <events>
      <event name="publish:itemProcessed">
        <handler type="Enterspeed.Source.SitecoreCms.V9.Events.PublishingEventHandler, Enterspeed.Source.SitecoreCms.V9" method="OnItemProcessed" resolve="true" />
      </event>
    </events>

    <pipelines>
      <initialize>
        <processor type="Enterspeed.Source.SitecoreCms.V9.Pipelines.InitializeEnterspeedPipeline, Enterspeed.Source.SitecoreCms.V9" resolve="true" />
      </initialize>
    </pipelines>
  </sitecore>
</configuration>